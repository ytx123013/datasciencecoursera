get_file_path <- function(directory, file_id) {
if (file_id < 10){
return(paste(directory,'/00',file_id,'.csv',sep = ''))
}else if(file_id < 100 && file_id >=10){
return(paste(directory,'/0',file_id,'.csv',sep = ''))
}else{
return(paste(directory,'/',file_id,'.csv',sep = ''))
}
}
